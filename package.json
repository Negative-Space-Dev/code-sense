{
  "name": "code-sense",
  "displayName": "Code Sense",
  "description": "quickly and visually understand your code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:code-sense.find-file-references",
    "onCommand:catCoding.start"
  ],
  "main": "./out/extension.js",
  "enabledApiProposals": [
    "findTextInFiles",
    "textSearchProvider"
  ],
  "contributes": {
    "commands": [
      {
        "command": "code-sense.find-file-references",
        "category": "Code Sense",
        "title": "Find current file references"
      },
      {
        "command": "catCoding.start",
        "title": "Start new cat coding session",
        "category": "Cat Coding"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/node-fetch": "^2.6.2",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vitejs/plugin-react": "^2.2.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.11.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "typescript": "^4.5.5",
    "vite": "^3.2.2"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "node-fetch": "^2.6.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
