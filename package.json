{
  "name": "code-sense",
  "displayName": "Code Sense",
  "description": "quickly and visually understand your code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:code-sense.find-file-references",
    "onCommand:code-sense.start"
  ],
  "main": "./out/extension.js",
  "enabledApiProposals": [
    "findTextInFiles",
    "textSearchProvider"
  ],
  "contributes": {
    "commands": [
      {
        "command": "code-sense.find-file-references",
        "category": "Code Sense",
        "title": "Find current file references"
      },
      {
        "command": "code-sense.start",
        "title": "Start code sense",
        "category": "Code Sense"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.11.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "path": "^0.12.7"
  }
}
